{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/sample-block",
  "version": "0.1.0",
  "title": "FAQブロック",
  "category": "text",
  "icon": "smiley",
  "description": "FAQブロックを追加します。実際はアコーディオン形式になります。",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "sample-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "attributes": {
    "question": {
      "type": "string",
      "source": "text",
      "selector": "dt",
      "default": ""
    },
    "answer": {
      "type": "string",
      "source": "html",
      "selector": "dd",
      "default": ""
    }
  }
}